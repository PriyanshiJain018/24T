// Helper function to find and show definition
window.findAndShowDefinition = function(term) {
    // First switch to definitions tab
    document.getElementById('definitions-content').classList.remove('hidden');
    document.querySelectorAll('.tab-content').forEach(content => {
        if (content.id !== 'definitions-content') {
            content.classList.add('hidden');
        }
    });
    document.querySelectorAll('.tab').forEach(tab => {
        tab.classList.remove('active');
    });
    document.querySelectorAll('.tab')[3].classList.add('active'); // Definitions tab
    
    // Then search for the term
    document.querySelector('#definitions-content .search-box').value = term;
    window.searchDefinitions(term);
};

// Helper function to get progress color
window.getProgressColor = function(percentage) {
    if (percentage === 0) return '#9CA3AF';
    if (percentage < 25) return '#10B981';
    if (percentage < 50) return '#F59E0B';
    if (percentage < 75) return '#F97316';
    return '#EF4444';
};
